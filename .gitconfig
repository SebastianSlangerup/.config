# This is Git's per-user configuration file.
[user]
	email = sebastian.slangerup1@gmail.com
	name = Sebastian Slangerup MÃ¸ller
[color]
	diff = auto
	status = auto
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[user]
	signingkey = /Users/sebastian/.ssh/id_ed25519.pub

[gpg]
	format = ssh

[gpg "ssh"]
  program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"

[commit]
  gpgsign = true

